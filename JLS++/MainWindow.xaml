<Window x:Class="JLS__.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JLS__"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Border x:Name="headline" BorderBrush="Black" BorderThickness="2" Height="80" VerticalAlignment="Top">
        <Grid x:Name="head">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="349*"/>
                <ColumnDefinition Width="145*"/>
            </Grid.ColumnDefinitions>
            <Label x:Name="time" Content="2020년 05월 04일 12시 53분 23초" Margin="10,4,0,4" FontFamily="Malgun Gothic" FontSize="30" FontWeight="Bold"/>
            <Border x:Name="profileline" BorderBrush="Black" BorderThickness="1,0,0,0" Grid.Column="1">
                <Grid x:Name="profile">
                    <Label x:Name="name" Content="프로필을 설정하세요." Margin="64,5,5,0" VerticalAlignment="Top" Height="39" FontSize="20" FontWeight="Bold"/>
                    <Button x:Name="setting" Content="설정" Margin="64,44,153,0" FontFamily="Malgun Gothic" FontSize="15" BorderBrush="{x:Null}" Foreground="#FF686868" Background="{x:Null}" Click="setting_Click" RenderTransformOrigin="-0.653,0.562"/>
                </Grid>
            </Border>
            <Grid HorizontalAlignment="Left" Height="76" Margin="185,170,0,-170" VerticalAlignment="Top" Width="100"/>
            <Grid x:Name="root" Height="488" Margin="0,0,0,-491" VerticalAlignment="Bottom" Grid.ColumnSpan="2">
                <Grid x:Name="settingPage" Visibility="Hidden">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="97*"/>
                        <ColumnDefinition Width="74*"/>
                        <ColumnDefinition Width="817*"/>
                    </Grid.ColumnDefinitions>
                    <Label x:Name="Whatsetting" Content="JLS++ 설정" Margin="10,10,10,0" VerticalAlignment="Top" FontFamily="Malgun Gothic Semilight" FontSize="25" Grid.Column="2" Height="47" FontWeight="Bold"/>
                    <Grid x:Name="greeting_setting" Grid.Column="2" HorizontalAlignment="Left" Height="431" Margin="10,57,0,0" VerticalAlignment="Top" Width="807" Visibility="Visible">
                        <Label Content="JLS++에 온걸 환영합니다!" HorizontalAlignment="Left" Margin="30,10,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="25" Height="47" Width="314"/>
                        <Label Content="JLS++는 정상어학원의 숙제 확인(그리고 말도안되는 숙제량)에 극대노한&#xD;&#xA;한 잼민이 프로그래머가 개발한 프로그램입니다.&#xD;&#xA;(숙제가 줄어드는게 더 좋겠지만)이 프로그램이 여러분의 과제 수행에&#xD;&#xA;도움이 되었으면 합니다!" HorizontalAlignment="Left" Margin="30,57,0,0" Height="111" VerticalAlignment="Top" FontFamily="Malgun Gothic" FontSize="14" Width="629"/>
                    </Grid>
                    <Grid x:Name="profileset" Grid.Column="2" HorizontalAlignment="Left" Height="431" Margin="10,57,0,0" VerticalAlignment="Top" Width="807" Visibility="Hidden">
                        <Label Content="이름 : " HorizontalAlignment="Left" Margin="30,6,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="60"/>
                        <Label Content="ID : " HorizontalAlignment="Left" Margin="30,44,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="42"/>
                        <Label Content="비밀번호 : " HorizontalAlignment="Left" Margin="30,82,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="94"/>
                        <Label Content="JLS++는 귀하의 개인정보를 소중하게 여깁니다.&#xD;&#xA;위와 같은 정보는 귀하의 기기에만 저장되며&#xD;&#xA;로그인시 JLS 서버 외에는 외부로 전송되지 않습니다." HorizontalAlignment="Left" Margin="10,127,0,0" Height="70" VerticalAlignment="Top" FontFamily="Malgun Gothic" FontSize="14" Width="345"/>
                        <TextBox x:Name="setName" HorizontalAlignment="Left" Height="33" Margin="120,6,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="308" FontFamily="Malgun Gothic" FontSize="17"/>
                        <TextBox x:Name="setID" HorizontalAlignment="Left" Height="33" Margin="120,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="308" FontFamily="Malgun Gothic" FontSize="17"/>
                        <PasswordBox x:Name="setPwd" HorizontalAlignment="Left" Height="33" Margin="120,82,0,0" VerticalAlignment="Top" Width="308" FontFamily="Malgun Gothic" FontSize="17"/>
                    </Grid>
                    <Grid x:Name="browserset" Grid.Column="2" HorizontalAlignment="Left" Height="431" Margin="10,57,0,0" VerticalAlignment="Top" Width="807" Visibility="Hidden">
                        <Label Content="브라우저 속성 설정" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="159"/>
                        <Label Content="User Agent : " HorizontalAlignment="Left" Margin="30,44,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="174"/>
                        <Label Content="언어 : " HorizontalAlignment="Left" Margin="30,108,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="174"/>
                        <Label Content="GPU 가속 : " HorizontalAlignment="Left" Margin="30,141,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="174"/>
                        <Label Content="창 보이기 : " HorizontalAlignment="Left" Margin="30,174,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="174"/>
                        <Label Content="가짜 플러그인 사용 : " HorizontalAlignment="Left" Margin="30,207,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="174"/>
                        <Label Content="이러한 정보는 웹서버에 Headless브라우저를 사용하지 않는 척 하기 위해 사용하는 설정들입니다.&#xD;&#xA;필요에 따라 설정하세요. 잘못 설정하면 프로그램이 제대로 구동하지 않을 수 있습니다." HorizontalAlignment="Left" Margin="10,245,0,0" Height="55" VerticalAlignment="Top" FontFamily="Malgun Gothic" FontSize="14" Width="629"/>
                        <TextBox x:Name="usragent" Height="63" Margin="204,44,40,0" TextWrapping="Wrap" VerticalAlignment="Top" FontFamily="Malgun Gothic" FontSize="17"/>
                        <TextBox x:Name="lang" Height="28" Margin="204,112,40,0" TextWrapping="Wrap" VerticalAlignment="Top" FontFamily="Malgun Gothic" FontSize="17"/>
                        <CheckBox x:Name="gpuac" Content="" HorizontalAlignment="Left" Margin="204,153,0,0" VerticalAlignment="Top" Height="16" Width="563" FontFamily="Malgun Gothic" FontSize="17"/>
                        <CheckBox x:Name="showWin" Content="" HorizontalAlignment="Left" Margin="204,186,0,0" VerticalAlignment="Top" Height="16" Width="563" FontFamily="Malgun Gothic" FontSize="17"/>
                        <CheckBox x:Name="fake_plugin" Content="" HorizontalAlignment="Left" Margin="204,219,0,0" VerticalAlignment="Top" Height="16" Width="563" FontFamily="Malgun Gothic" FontSize="17"/>
                    </Grid>
                    <Grid x:Name="fileset" Grid.Column="2" HorizontalAlignment="Left" Height="431" Margin="10,57,0,0" VerticalAlignment="Top" Width="807" Visibility="Hidden">
                        <Label Content="DATABASE 설정" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="159"/>
                        <Label Content="저장 경로 :" HorizontalAlignment="Left" Margin="30,44,0,0" VerticalAlignment="Top" FontFamily="Malgun Gothic" RenderTransformOrigin="0.744,1.462" FontSize="17" Height="33" Width="100"/>
                        <Label Content="JLS++의 모든 정보는 데이터베아스로 관리됩니다.&#xD;&#xA;위의 설정으로 데이터베이스를 직접 관리할 수 있습니다." HorizontalAlignment="Left" Margin="10,120,0,0" Height="55" VerticalAlignment="Top" FontFamily="Malgun Gothic" FontSize="14" Width="629"/>
                        <TextBox x:Name="savePath" Height="29" Margin="130,48,40,0" TextWrapping="Wrap" VerticalAlignment="Top" FontFamily="Malgun Gothic" FontSize="17" IsEnabled="False"/>
                        <Button x:Name="clearSet" Content="설정 초기화" Margin="30,82,40,0" VerticalAlignment="Top" Height="33" FontFamily="Malgun Gothic" FontSize="17" Background="{x:Null}" Click="Button_Click"/>
                    </Grid>
                    <Border BorderBrush="Black" BorderThickness="2,0" Grid.ColumnSpan="2" Margin="-2,-2,2,0">
                        <Grid>
                            <Button x:Name="account" Content="프로필 설정" Height="39" VerticalAlignment="Top" BorderBrush="#FF707070" BorderThickness="0,0,0,1" FontFamily="Malgun Gothic" FontSize="20" Background="{x:Null}" Click="account_Click"/>
                            <Button x:Name="browser" Content="브라우저 설정" Height="39" VerticalAlignment="Top" BorderBrush="#FF707070" BorderThickness="0,0,0,1" FontFamily="Malgun Gothic" FontSize="20" Background="{x:Null}" Click="browser_Click" Margin="0,39,0,0"/>
                            <Button x:Name="file" Content="스토리지 설정" Height="39" VerticalAlignment="Top" BorderBrush="#FF707070" BorderThickness="0,0,0,1" FontFamily="Malgun Gothic" FontSize="20" Background="{x:Null}" Click="file_Click" Margin="0,78,0,0"/>
                        </Grid>
                    </Border>
                </Grid>
                <Grid Margin="133,114,755,274"/>
                <Border x:Name="r2" BorderBrush="Black" BorderThickness="2,0,2,2" Margin="-2,-1,-2,0">
                    <Grid/>
                </Border>
            </Grid>
        </Grid>
    </Border>
</Window>
